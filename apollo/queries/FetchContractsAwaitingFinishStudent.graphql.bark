query FetchContractsAwaitingFinishStudent($studentId: Int!) {
  findManyContractNotFinishedByStudent(studentId: $studentId) {
    id
    start
    end
    name
    rgb
    skills(markValues: ["TODO", "AWAITING_CORRECTION", "TO_FINISH"]) {
      id
      name
      skillToStudent(studentId: $studentId) {
        mark {
          text
          value
          rgb
          icon
        }
      }
    }
  }
}



